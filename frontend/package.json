{
  "name": "ckan-ui",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --modern",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "babel-runtime": "^6.26.0",
    "core-js": "^2.6.12",
    "es6-promise": "^4.2.8",
    "marked": "^0.7.0",
    "material-design-icons-iconfont": "^4.0.5",
    "moment": "^2.29.2",
    "sass": "^9.0.1",
    "pdfvuer": "1.9.2",
    "stylus": "^0.54.8",
    "stylus-loader": "^3.0.2",
    "v-clipboard": "^2.2.3",
    "vee-validate": "^3.4.5",
    "vue": "^2.7.16",
    "vue-config": "^1.0.0",
    "vue-google-charts": "^0.3.3",
    "vue-gtag": "1.16.1",
    "vue-i18n": "^8.24.3",
    "vue-infinite-loading": "^2.4.5",
    "vue-line-clamp": "^1.3.2",
    "vue-router": "^3.5.1",
    "vue-scrollto": "^2.20.0",
    "vuetify": "2.3.10",
    "vuetify-loader": "1.6.0",
    "vuex": "^3.6.2",
    "vuex-persistedstate": "^2.7.1"
  },
  "devDependencies": {
    "@mdi/font": "^4.9.95",
    "@vue/cli-plugin-babel": "^3.12.1",
    "@vue/cli-plugin-eslint": "^3.12.1",
    "@vue/cli-plugin-unit-mocha": "~4.5.0",
    "@vue/cli-service": "^4.5.13",
    "@vue/test-utils": "^1.0.3",
    "babel-eslint": "^10.1.0",
    "chai": "^4.1.2",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.2.3",
    "sass": "^1.32.11",
    "sass-loader": "^7.3.1",
    "vue-template-compiler": "^2.6.12"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-unused-vars": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "mocha": true
        }
      }
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
